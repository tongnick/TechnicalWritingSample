 fingerprints."}],"type":"topic","kind":"symbol","role":"symbol","url":"\/documentation\/bcgbiometry\/biometrynonecaseview","navigatorTitle":[{"kind":"identifier","text":"BiometryNoneCaseView"}]},"doc://com.bcg.biometry/documentation/BCGBiometry/BiometryTouchIDCaseView":{"title":"BiometryTouchIDCaseView","type":"topic","navigatorTitle":[{"text":"BiometryTouchIDCaseView","kind":"identifier"}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"BiometryTouchIDCaseView"}],"identifier":"doc:\/\/com.bcg.biometry\/documentation\/BCGBiometry\/BiometryTouchIDCaseView","kind":"symbol","role":"symbol","abstract":[{"text":"The ","type":"text"},{"code":"BiometryTouchIDCaseView","type":"codeVoice"},{"type":"text","text":" struct defines a SwiftUI view that provides users with information and next steps after enabling Touch ID on their macOS device. The view includes dynamic content that informs users about the successful activation of Touch ID and guides them on what to do next, such as enabling TouchID as a factor in Okta Verify."}],"url":"\/documentation\/bcgbiometry\/biometrytouchidcaseview"},"doc://com.bcg.biometry/documentation/BCGBiometry":{"type":"topic","url":"\/documentation\/bcgbiometry","abstract":[{"type":"text","text":"for macOS"}],"title":"BCGBiometry","role":"collection","kind":"symbol","identifier":"doc:\/\/com.bcg.biometry\/documentation\/BCGBiometry","images":[{"type":"icon","identifier":"1-BCGBiometryIcon.png"}]}}}